<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- solid表示远的填充色 -->
    <solid android:color="#0BE666" />
    <!-- stroke则代表远的边框线 -->
    <stroke
        android:width="3dp"
        android:color="#179BE78F" />

    <size android:width="13dp" android:height="13dp"></size>
</shape>