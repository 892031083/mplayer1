<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_enabled="false">
    <shape android:shape="oval">
        <stroke android:color="@color/dot_enabled" android:width="0.1dp"></stroke>
        <size android:height="8dp" android:width="8dp"></size>
        <solid android:color="@color/dot_"></solid>
    </shape>
</item>

<item android:state_enabled="true">
    <shape android:shape="oval">
        <stroke android:color="@color/dot_enabled" android:width="0.1dp"></stroke>
        <size android:height="8dp" android:width="8dp"></size>
        <solid android:color="@color/dot_enabled"></solid>
    </shape>
</item>
</selector>